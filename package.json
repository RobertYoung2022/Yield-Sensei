{
  "name": "yield-sensei",
  "version": "0.1.0",
  "description": "Multi-Agent DeFi Investment Advisor with 8 Satellite Systems",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run build:ts && npm run build:rust",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts:watch": "tsc --project tsconfig.build.json --watch",
    "build:rust": "cd src/satellites/forge && cargo build --release",
    "build:wasm": "cd src/core/orchestration/state && wasm-pack build --target web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --project tsconfig.typecheck.json",
    "typecheck:build": "tsc --project tsconfig.build.json --noEmit",
    "type-validate": "tsx scripts/type-validation.ts",
    "type-validate:progressive": "tsx scripts/type-validation.ts --mode=progressive",
    "type-validate:report": "tsx scripts/type-validation.ts --report",
    "type-coverage": "type-coverage --detail",
    "type-coverage:check": "type-coverage --at-least 85",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "docs:generate": "typedoc src --out docs/api",
    "security:audit": "npm audit",
    "performance:benchmark": "node scripts/benchmark.js",
    "validate-env": "tsx scripts/validate-env.ts",
    "validate-env:dev": "tsx scripts/validate-env.ts -- --environment=development",
    "validate-env:staging": "tsx scripts/validate-env.ts -- --environment=staging",
    "validate-env:prod": "tsx scripts/validate-env.ts -- --environment=production",
    "secrets:store": "tsx scripts/manage-secrets.ts store",
    "secrets:get": "tsx scripts/manage-secrets.ts get",
    "secrets:rotate": "tsx scripts/manage-secrets.ts rotate",
    "secrets:list": "tsx scripts/manage-secrets.ts list",
    "secrets:delete": "tsx scripts/manage-secrets.ts delete",
    "secrets:schedule-rotation": "tsx scripts/manage-secrets.ts schedule-rotation",
    "secrets:process-rotations": "tsx scripts/manage-secrets.ts process-rotations",
    "secrets:health": "tsx scripts/manage-secrets.ts health",
    "secrets:audit": "tsx scripts/manage-secrets.ts audit",
    "secrets:users": "tsx scripts/manage-secrets.ts users",
    "secrets:roles": "tsx scripts/manage-secrets.ts roles",
    "test:rwa": "tsx scripts/test-rwa-scoring.ts",
    "test:rwa:demo": "tsx tests/satellites/sage/rwa-integration-demo.ts",
    "test:rwa:unit": "jest tests/satellites/sage/rwa-opportunity-scoring.test.ts --verbose --testTimeout=30000",
    "encryption:validate": "tsx src/security/encryption/validation-runner.ts validate",
    "encryption:validate-quick": "tsx src/security/encryption/validation-runner.ts quick",
    "encryption:report": "tsx src/security/encryption/validation-runner.ts report",
    "encryption:test": "jest src/security/encryption/__tests__/",
    "ci:encryption": "npm run encryption:validate-quick && npm run encryption:validate",
    "env:validate": "tsx src/security/environment/env-runner.ts validate",
    "env:validate-quick": "tsx src/security/environment/env-runner.ts quick",
    "env:report": "tsx src/security/environment/env-runner.ts report",
    "env:scan": "tsx src/security/environment/env-runner.ts scan",
    "env:test": "jest tests/security/environment/",
    "ci:env": "npm run env:validate-quick && npm run env:validate",
    "db:security:validate": "tsx src/security/database/db-security-runner.ts validate",
    "db:security:quick": "tsx src/security/database/db-security-runner.ts quick",
    "db:security:report": "tsx src/security/database/db-security-runner.ts report",
    "db:security:test": "jest tests/security/database/",
    "db:security:simple": "tsx src/security/database/simple-db-runner.ts",
    "db:security:test-simple": "jest tests/security/database/db-security-validator-simple.test.ts",
    "ci:db:security": "npm run db:security:quick && npm run db:security:validate",
    "packages:update": "tsx scripts/update-packages.ts",
    "packages:update:safe": "tsx scripts/update-packages.ts --mode=safe",
    "packages:update:security": "tsx scripts/update-packages.ts --mode=security-only",
    "packages:update:dry-run": "tsx scripts/update-packages.ts --mode=safe --dry-run",
    "packages:update:selective": "tsx scripts/selective-package-update.ts",
    "packages:update:security-only": "tsx scripts/selective-package-update.ts --security-only",
    "packages:audit": "tsx scripts/security-package-audit.ts",
    "security:packages": "npm run packages:audit && npm run packages:update:security",
    "type-report": "tsx scripts/type-error-reporter.ts",
    "type-report:trends": "tsx scripts/type-error-reporter.ts --trends",
    "type-report:trends-only": "tsx scripts/type-error-reporter.ts --trends-only",
    "ci:type-safety": "npm run typecheck && npm run type-coverage:check && npm run type-report",
    "prepare": "husky"
  },
  "keywords": [
    "defi",
    "ai",
    "multi-agent",
    "yield-farming",
    "cross-chain",
    "investment",
    "advisor",
    "cryptocurrency"
  ],
  "author": "YieldSensei Team",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^1.12.0",
    "@graphql-tools/merge": "^9.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@tensorflow/tfjs-node": "^4.14.0",
    "@types/ioredis": "^4.28.10",
    "@types/socket.io": "^3.0.1",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "commander": "^11.1.0",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dataloader": "^2.2.3",
    "decimal.js": "^10.4.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.8.0",
    "express": "^4.18.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-request-id": "^3.0.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "express-winston": "^4.2.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.20",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.0",
    "node-cron": "^4.2.1",
    "opossum": "^8.1.3",
    "pg": "^8.11.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^3.0.6",
    "redis": "^4.6.15",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "ws": "^8.14.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.0",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knex": "^3.0.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.3",
    "type-coverage": "^2.29.7",
    "typedoc": "^0.25.0",
    "typescript": "^5.4.5",
    "wasm-pack": "^0.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yield-sensei.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/yield-sensei/issues"
  },
  "homepage": "https://github.com/yourusername/yield-sensei#readme",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "bash -c 'npm run typecheck'"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
