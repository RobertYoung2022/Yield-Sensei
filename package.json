{
  "name": "yield-sensei",
  "version": "0.1.0",
  "description": "Multi-Agent DeFi Investment Advisor with 8 Satellite Systems",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run build:rust",
    "build:rust": "cd src/satellites/forge && cargo build --release",
    "build:wasm": "cd src/core/orchestration/state && wasm-pack build --target web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "docs:generate": "typedoc src --out docs/api",
    "security:audit": "npm audit",
    "performance:benchmark": "node scripts/benchmark.js"
  },
  "keywords": [
    "defi",
    "ai",
    "multi-agent",
    "yield-farming",
    "cross-chain",
    "investment",
    "advisor",
    "cryptocurrency"
  ],
  "author": "YieldSensei Team",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@tensorflow/tfjs-node": "^4.14.0",
    "axios": "^1.6.0",
    "compression": "^1.7.0",
    "cors": "^2.8.0",
    "decimal.js": "^10.4.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.8.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "kafkajs": "^2.2.0",
    "node-cron": "^3.0.0",
    "opossum": "^8.1.3",
    "pg": "^8.11.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.6.15",
    "uuid": "^9.0.0",
    "winston": "^3.13.1",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.10.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "knex": "^3.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^3.14.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.5.3",
    "wasm-pack": "^0.13.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yield-sensei.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/yield-sensei/issues"
  },
  "homepage": "https://github.com/yourusername/yield-sensei#readme"
}
