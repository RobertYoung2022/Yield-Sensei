{
  "name": "yield-sensei",
  "version": "0.1.0",
  "description": "Multi-Agent DeFi Investment Advisor with 8 Satellite Systems",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run build:rust",
    "build:rust": "cd src/satellites/forge && cargo build --release",
    "build:wasm": "cd src/core/orchestration/state && wasm-pack build --target web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "docs:generate": "typedoc src --out docs/api",
    "security:audit": "npm audit",
    "performance:benchmark": "node scripts/benchmark.js",
    "validate-env": "tsx scripts/validate-env.ts",
    "validate-env:dev": "tsx scripts/validate-env.ts -- --environment=development",
    "validate-env:staging": "tsx scripts/validate-env.ts -- --environment=staging",
    "validate-env:prod": "tsx scripts/validate-env.ts -- --environment=production",
    "secrets:store": "tsx scripts/manage-secrets.ts store",
    "secrets:get": "tsx scripts/manage-secrets.ts get",
    "secrets:rotate": "tsx scripts/manage-secrets.ts rotate",
    "secrets:list": "tsx scripts/manage-secrets.ts list",
    "secrets:delete": "tsx scripts/manage-secrets.ts delete",
    "secrets:schedule-rotation": "tsx scripts/manage-secrets.ts schedule-rotation",
    "secrets:process-rotations": "tsx scripts/manage-secrets.ts process-rotations",
    "secrets:health": "tsx scripts/manage-secrets.ts health",
    "secrets:audit": "tsx scripts/manage-secrets.ts audit",
    "secrets:users": "tsx scripts/manage-secrets.ts users",
    "secrets:roles": "tsx scripts/manage-secrets.ts roles",
    "test:rwa": "tsx scripts/test-rwa-scoring.ts",
    "test:rwa:demo": "tsx tests/satellites/sage/rwa-integration-demo.ts",
    "test:rwa:unit": "jest tests/satellites/sage/rwa-opportunity-scoring.test.ts --verbose --testTimeout=30000"
  },
  "keywords": [
    "defi",
    "ai",
    "multi-agent",
    "yield-farming",
    "cross-chain",
    "investment",
    "advisor",
    "cryptocurrency"
  ],
  "author": "YieldSensei Team",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/merge": "^9.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.52.0",
