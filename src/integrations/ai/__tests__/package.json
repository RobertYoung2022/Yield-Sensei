{
  "name": "@yieldsensei/ai-integration-tests",
  "version": "1.0.0",
  "description": "Comprehensive testing suite for YieldSensei AI service integration layer",
  "scripts": {
    "test": "jest",
    "test:unit": "jest unit/",
    "test:integration": "jest integration/",
    "test:performance": "jest performance/",
    "test:security": "jest security/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:performance-detailed": "NODE_ENV=performance jest performance/ --verbose",
    "test:load": "jest performance/load-testing.test.ts --verbose",
    "test:security-audit": "jest security/ --verbose",
    "lint": "eslint '**/*.ts' --fix",
    "clean": "rm -rf coverage/ .nyc_output/ test-results/",
    "report": "jest --coverage --coverageReporters=html lcov text",
    "benchmark": "node scripts/benchmark.js"
  },
  "keywords": [
    "ai",
    "testing",
    "integration",
    "performance",
    "security",
    "openai",
    "anthropic",
    "perplexity",
    "yieldsensei"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/setup.ts"],
    "testSequencer": "<rootDir>/sequencer.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "YieldSensei Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yieldsensei/yieldsensei.git",
    "directory": "src/integrations/ai/__tests__"
  },
  "bugs": {
    "url": "https://github.com/yieldsensei/yieldsensei/issues"
  },
  "homepage": "https://github.com/yieldsensei/yieldsensei#readme"
}